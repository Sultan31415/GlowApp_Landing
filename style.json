    "backgroundGradients": [
      "bg-gradient-to-r from-purple-600 to-pink-600",
      "hover:from-purple-700 hover:to-pink-700"
    ],
  
    "typography": {
      "heading1": { "fontSize": ["text-6xl", "md:text-7xl"], "fontWeight": "font-extrabold", "color": "text-gray-900" },
      "heading2": { "fontSize": "text-4xl", "fontWeight": "font-bold", "color": "text-gray-900" },
      "paragraph": { "fontSize": ["text-xl", "md:text-2xl"], "color": "text-gray-600", "fontWeight": "font-light" }
    },
  
    "shapes": {
      "roundedFull": "rounded-full",
      "rounded3xl": "rounded-3xl",
      "rounded4xl": "rounded-4xl"
    },
  
    "effects": {
      "shadows": ["shadow-md", "shadow-lg", "hover:shadow-xl", "shadow-purple-500/25", "hover:shadow-purple-500/30", "shadow-gray-500/5"],
      "transforms": ["transform", "hover:scale-105", "group-hover:translate-x-1", "hover:-translate-y-2"],
      "transitions": ["transition-all", "duration-300", "duration-500"]
    }
  }